{
    "name": "Nodue",
    "author": "Timon Schenzel",
    "license": "MIT",
    "private": true,
    "bin": {
        "tap-nodue": "./jsunit/tap-nodue.js"
    },
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "nodue": "node index.js",
        "nodue-dev": "nodemon --watch routes.js --watch app --watch config --watch Nodue/src index.js -- dev",
        "nodue:dev": "concurrently \"npm run nodue-dev hot\" \"npm run webpack-silent\"",
        "testskarma": "karma start karma.conf.js --log-level debug --single-run",
        "nodue-tab": "./jsunit/tap-nodue.js",
        "test": "node node_modules/ava/cli.js jsunit/jsunit.js --tap | tap-nodue || true",
        "tests": "node node_modules/ava/cli.js jsunit/jsunit.js --tap | tap-nodue || true"
    },
    "devDependencies": {
        "ava": "^0.22.0",
        "cheerio": "^1.0.0-rc.3",
        "cross-env": "^5.2.1",
        "ouch": "^1.1.0",
        "resolve-url-loader": "^2.3.2",
        "sass": "^1.22.12",
        "sass-loader": "^7.3.1",
        "tap-dot": "^1.0.5",
        "tap-spec": "^4.1.2",
        "vue-template-compiler": "^2.6.10"
    },
    "dependencies": {
        "auto-loader": "^0.2.0",
        "axios": "^0.15.3",
        "bookshelf": "^0.15.1",
        "bulma": "^0.2.3",
        "chalk": "^1.1.3",
        "chokidar": "^1.6.1",
        "collect.js": "^3.1.2",
        "concurrently": "^3.6.1",
        "deepmerge": "^1.5.2",
        "diff": "^3.5.0",
        "dotenv": "^4.0.0",
        "express": "^4.17.1",
        "figures": "^2.0.0",
        "graphql-request": "^1.8.2",
        "jquery": "^3.5.0",
        "js-object-pretty-print": "^0.3.0",
        "knex": "^0.12.6",
        "laravel-mix": "^4.1.4",
        "lodash": "^4.17.15",
        "moment": "^2.24.0",
        "mysql": "^2.17.1",
        "pg": "^6.4.2",
        "pluralize": "^3.1.0",
        "serve-favicon": "^2.5.0",
        "socket.io": "^1.7.2",
        "socket.io-client": "^1.7.2",
        "sugar": "^2.0.6",
        "tailwindcss": "^1.1.2",
        "tap-min": "^1.3.0",
        "traceback": "^0.3.1",
        "vue": "^2.6.10",
        "vue-server-renderer": "^2.4.4",
        "vue-timeago": "^3.4.4",
        "vuex": "^2.5.0"
    }
}
